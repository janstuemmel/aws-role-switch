{
  "name": "aws-role-switch",
  "private": "true",
  "version": "0.6.0",
  "license": "MIT",
  "scripts": {
    "test": "jest --colors",
    "lint": "eslint . --cache",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "ladle": "ladle serve",
    "watch": "./scripts/build.ts --watch",
    "build": "./scripts/build.ts --minify",
    "bundle": "run-s clean build ext:build:firefox ext:build:chrome",
    "ext:run:firefox": "web-ext run -s dist/firefox",
    "ext:lint:firefox": "web-ext lint -s dist/firefox",
    "ext:build:firefox": "web-ext build -s dist/firefox -n firefox.zip -a dist",
    "ext:run:chrome": "web-ext run -s dist/chrome --target chromium",
    "ext:lint:chrome": "web-ext lint -s dist/chrome",
    "ext:build:chrome": "web-ext build -s dist/chrome -n chrome.zip -a dist"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.5.1",
    "@codemirror/autocomplete": "^6.3.4",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/language": "^6.3.1",
    "@codemirror/legacy-modes": "^6.1.0",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.6.0",
    "@uiw/react-codemirror": "^4.15.1",
    "buffer": "^6.0.3",
    "colortranslator": "^1.9.2",
    "js-ini": "^1.5.1",
    "marked": "^4.0.17",
    "normalize.css": "^8.0.1",
    "outmatch": "^0.7.0",
    "pako": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@ladle/react": "^2.4.5",
    "@types/chrome": "^0.0.202",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.191",
    "@types/marked": "^4.0.3",
    "@types/pako": "^2.0.0",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "esbuild": "^0.14.49",
    "esbuild-plugin-copy": "^1.3.0",
    "esbuild-sass-plugin": "^2.2.6",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-newline": "^1.5.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "web-ext": "^7.1.0"
  }
}
